{
  "manifest_version": 2,
  "name": "TronWallet",
  "description": "Your First Smart Digital Tron Wallet",
  "version": "0.0.1",
  "icons": {
    "16": "static/imgs/tron.png",
    "48": "static/imgs/tron.png",
    "128": "static/imgs/tron.png"
  },
  "browser_action": {
    "default_icon": "static/imgs/tron.png",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "static/js/jquery.min.js"
    ]
  },
  "externally_connectable": {
    "matches": [
    ]
  },
  "content_scripts": [
  ],
  "web_accessible_resources": [
    "static/*"
  ],
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "notifications",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "<all_urls>"
  ]
}